{
  "builds": [
    {
      "src": "hbd/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "hbd/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "hbd.settings"
  }
}